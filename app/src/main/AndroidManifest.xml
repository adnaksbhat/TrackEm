<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trackem">

    <application
        android:allowBackup="true"
        android:icon="@drawable/pigpng"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pigpng"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Ratings"></activity>
        <activity android:name=".NotificationView" />
        <activity android:name=".ThisPieChart" />
        <activity android:name=".ThisBarChart" />
        <activity android:name=".ShowData2" />
        <activity android:name=".ShowData" />
        <activity android:name=".AddExpense" />
        <activity android:name=".AddIncome" />
        <activity android:name=".Convertor" />
        <activity android:name=".Feedback" />
        <activity android:name=".Plate2" />
        <activity android:name=".SelectImageActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".Register" />
        <activity android:name=".MainActivity" />
        <activity android:name=".tab1" />
        <activity android:name=".tab2" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_place_api" />

        <service android:name=".MyFirebaseInstanceService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>